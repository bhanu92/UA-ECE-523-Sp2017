@article{Kuczynski2010Nature,
	Annote = {http://www.nature.com/nmeth/journal/v7/n10/full/nmeth.1499.html},
	Author = {Justin Kuczynski and Zongzhi Liu and Catherine Lozupone and Daniel McDonald and Noah Fierer and Rob Knight},
	Date-Added = {2012-06-09 01:04:10 +0000},
	Date-Modified = {2014-03-05 21:44:05 +0000},
	Journal = {Nature Methods},
	Keywords = {PCoA, metagenomics},
	Number = {10},
	Pages = {813--821},
	Review = {Paper seeks to answer: Do the methods that generally work well for gradient analysis in ecological systems also work well for cluster detection? Observations (Gradient) --increasing the number of sequences per sample does not necessarily lead to an improvement in the ability to detect ecological patterns --simulated subtle gradients were revealed at greater numbers of sequences per sample suggesting that investigation of more subtle effects requires deeper sequencing. --Qualitative methods (either the species was there or it was not) generally performed worse than quantitative (abundance-weighted) methods Generating Clusters --partitioned samples into discrete clusters. To simulate this, we formed a hypothetical sample at the root of a hierarchy, which defined the relatedness of samples both inter- and intra-cluster --communities were clustered into discrete communities. Observations (Clustering) --relative efficacy of different analyses was dependent on the relatedness of clusters and that different analysis techniques applied to the same data were of substantially different effectiveness at revealing the underlying clusters --some measures that work well on gradient data, fail to accuractely identify the distinct clusters --the recommended analysis methodology depended on the degree of separation between the underlying clusters, and that even excessive sequencing did not provide reasonable resolution if we chose the wrong analytical method Cluster Quality: fraction of sample pairs that are correctly identified as belonging to the same or different clusters Questions --why does the keyboard dataset produce three distinct clusters? what do these clusters represent? --why implement pco with different distance measures? pca works off the covariance matrix Future: --Resolving the arch effect so that multiple gradients can be studied remains an important challenge for the field.},
	Title = {Microbial community resemblance methods differ in their ability to detect biologically relevant patterns},
	Volume = {7},
	Year = {2010},
